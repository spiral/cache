{
    "name": "spiral/cache",
    "type": "library",
    "description": "Common Cache Interfaces",
    "license": "MIT",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/framework/issues",
        "source": "https://github.com/spiral/core"
    },
    "authors": [
        {
            "name": "Anton Titov (wolfy-j)",
            "email": "wolfy-j@spiralscout.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "spiral/core": "^2.14.1",
        "spiral/config": "^2.14.1",
        "spiral/files": "^2.14.1",
        "psr/simple-cache": "1 - 2"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\Cache\\": "src"
        }
    },
    "require-dev": {
        "spiral/boot": "^2.14.1",
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^8.5|^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\Tests\\Cache\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.15.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
